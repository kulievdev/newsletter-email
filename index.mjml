<mjml>
    <mj-head>
        <mj-style> * { box-sizing: border-box; } </mj-style>
        <mj-style inline="inline"></mj-style>
    </mj-head>
    <mj-body background-color="#F2F2F2">
        <!-- Header Section -->
        <mj-section padding="12px">
            <mj-column width="85%">
                <mj-text
                    color="#666666"
                    font-size="12px"
                    align="left"
                    padding="0"
                >
                    <span style="font-weight: bold">Alex</span> - There’s no
                    impact to your credit score if you’re declined.
                </mj-text>
            </mj-column>
            <mj-column width="15%">
                <mj-text font-size="13px" align="right" padding="0">
                    <a
                        style="color: #666666"
                        target="_blank"
                        href="https://view.emails.paypal.com/?qs=8e54b9243bf18d10d588303118257b7cbff80c20cbedcaab8de4d3cc8380f696e866aa26b082a8d735e346dac2d368e38caee0fa17e2fb8672ecb03c51f24335bd6a1344f0c01f59b8c8baf743a02c4d"
                        >View Online
                    </a>
                </mj-text>
            </mj-column>
        </mj-section>
        <mj-section background-color="#FEFEFE" padding="0">
            <mj-column>
                <mj-image
                    padding="26px 0"
                    align="center"
                    width="148px"
                    alt="paypal text logo"
                    src="./assets/text-logo.png"
                    href=""
                />
            </mj-column>
        </mj-section>
        <!-- Put your money back Section -->
        <mj-section background-color="#000" padding="0">
            <mj-column>
                <mj-text
                    font-size="48px"
                    color="#fff"
                    padding="80px 120px"
                    align="center"
                    font-weight="500"
                >
                    Put money back <br />
                    in your pocket with up to 3% <br />
                    cash back.
                </mj-text>
            </mj-column>
        </mj-section>
        <!-- Apply Now-Top Section -->
        <mj-section padding="0">
            <mj-column background-color="#fff" padding="0">
                <mj-text
                    font-size="16px"
                    align="center"
                    padding="30px 40px"
                    line-height="22px"
                >
                    If approved, you’ll earn unlimited 3% cash back on all the
                    brands you love when you check out with PayPal. Plus, earn
                    1.5% on all other Mastercard purchases.<sup>1</sup>
                </mj-text>
                <mj-button
                    background-color="#000"
                    color="#fff"
                    font-size="18px"
                    inner-padding="16px 30px"
                    border-radius="50px"
                    align="center"
                    padding="0 0 20px 0"
                    href="https://www.paypal.com/signin?returnUri=https%3A%2F%2Fwww.paypal.com%2Fcredit-application&state=%252Fco%253Fpresent%253DclpCOB32CONTROLSIPRIMB%2526track%253DcltNHATESTCREATIVE9102024SYI5%2526utm_source%253Dsfmc%2526utm_medium%253Demail%2526utm_campaign%253DOW_EM_AH_MC_NI_NI_202409_458432_cltNHATESTCREATIVE9102024SYI5%2526sfmc_id%253D86C4TS2YER58Q%2526utm_content%253DApply_Now_CTA1&onboardData=%7B%22product_flow%22%3A%22cobrand%22%2C%22intent%22%3A%22credit%22%2C%22signUpRequest%22%3A%7B%22method%22%3A%22GET%22%2C%22url%22%3A%22https%3A%2F%2Fwww.paypal.com%2Fwelcome%2Fflow%2Fcredit%3Fflow%%22%7D%7D&forceLogin=true"
                >
                    Apply Now
                </mj-button>
                <mj-image
                    alt="phone"
                    src="./assets/phone.png"
                    href="https://www.paypal.com/signin?returnUri=https%3A%2F%2Fwww.paypal.com%2Fcredit-application&state=%252Fco%253Fpresent%253DclpCOB32CONTROLSIPRIMB%2526track%253DcltNHATESTCREATIVE9102024SYI5%2526utm_source%253Dsfmc%2526utm_medium%253Demail%2526utm_campaign%253DOW_EM_AH_MC_NI_NI_202409_458432_cltNHATESTCREATIVE9102024SYI5%2526sfmc_id%253D86C4TS2YER58Q%2526utm_content%253DHero_Image_2&onboardData=%7B%22product_flow%22%3A%22cobrand%22%2C%22intent%22%3A%22credit%22%2C%22signUpRequest%22%3A%7B%22method%22%3A%22GET%22%2C%22url%22%3A%22https%3A%2F%2Fwww.paypal.com%2Fwelcome%2Fflow%2Fcredit%3Fflow%%3D%3D%22%7D%7D&forceLogin=true"
                />
            </mj-column>
        </mj-section>
        <!-- Major Rewards Section -->
        <mj-wrapper background-color="#F6F7FA" padding="0">
            <mj-section padding="0">
                <mj-column>
                    <mj-text
                        padding="30px 0"
                        font-size="22px"
                        font-weight="bold"
                        align="center"
                    >
                        Cash in on major rewards.
                    </mj-text>
                </mj-column>
            </mj-section>
            <mj-section padding="0">
                <mj-column>
                    <mj-image
                        padding="0"
                        align="center"
                        width="38px"
                        alt="credit card"
                        src="./assets/credit-card.png"
                    />
                    <mj-text
                        padding="10px 0"
                        font-size="18px"
                        font-weight="500"
                        align="center"
                    >
                        No credit score impact if declined.<sup>2</sup>
                    </mj-text>
                    <mj-text padding="10px 0" font-size="16px" align="center">
                        Plus, get a decision in seconds.
                    </mj-text>
                </mj-column>
            </mj-section>
            <mj-section padding="28px">
                <mj-column>
                    <mj-image
                        padding="0"
                        align="center"
                        width="38px"
                        alt="rotate icon"
                        src="./assets/rotate.png"
                    />
                    <mj-text
                        padding="10px 0"
                        font-size="18px"
                        font-weight="500"
                        align="center"
                    >
                        No rotating categories or annual fee.<sup>3</sup>
                    </mj-text>
                    <mj-text padding="10px 0" font-size="16px" align="center">
                        There’s no limit on how much cash back you can earn.
                    </mj-text>
                </mj-column>
            </mj-section>
            <mj-section padding="0 0 30px 0">
                <mj-column>
                    <mj-image
                        padding="0"
                        align="center"
                        width="38px"
                        alt="dollar sign"
                        src="./assets/dollar-sign.png"
                    />
                    <mj-text
                        padding="10px 0"
                        font-size="18px"
                        font-weight="500"
                        align="center"
                    >
                        Redeem your cash back.
                    </mj-text>
                    <mj-text
                        padding="10px 60px 0 60px"
                        font-size="16px"
                        align="center"
                    >
                        Redeem cash back to your PayPal balance<sup>4</sup> as
                        soon as purchases post to your account.
                    </mj-text>
                </mj-column>
            </mj-section>
        </mj-wrapper>
        <!-- Apply Now-Bottom Section -->
        <mj-section background-color="#fff" padding="30px 0">
            <mj-column>
                <mj-text
                    padding="0 0 30px 0"
                    font-size="24px"
                    font-weight="500"
                    align="center"
                    >Earn with the PayPal Cashback Mastercard.
                </mj-text>
                <mj-button
                    background-color="#000"
                    color="#fff"
                    font-size="18px"
                    inner-padding="16px 30px"
                    border-radius="50px"
                    align="center"
                    padding="0"
                >
                    Apply Now
                </mj-button>
            </mj-column>
        </mj-section>
        <!-- Slogan Section -->
    </mj-body>
</mjml>
